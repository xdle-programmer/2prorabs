mixin orderProduct
    .account__order-item
        .account__order-item-img-wrapper
            img(src='/local/templates/stroygip/ts/images/examples/product-1.jpg').account__order-item-img
        .account__order-item-desc
            .account__order-item-desc-title
                a(href='#').account__order-item-desc-title-item Газонокосилка электрическая huter elm-1100 70/4/2
                .account__order-item-desc-title-count
                    .account__order-item-desc-title-count-text На складе:
                    .account__order-item-desc-title-count-number 42 шт.
            .account__order-item-desc-price
                .account__order-item-desc-price-number 122 342
                .account__order-item-desc-price-currency сом

mixin orderProductNot
